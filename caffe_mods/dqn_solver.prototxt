net: "caffe_mods/dqn_new.prototxt"
solver_type: ADADELTA
momentum: 0.90
base_lr: 0.002
lr_policy: "step"
gamma: 0.1
stepsize: 1000000
max_iter: 2000000
display: 10000

# you should only see a lot of clip message a the beginning of training
# for about a second, otherwise occasionally you'll see some if things
# are working
clip_gradients: 100

# snapshot intermediate results
snapshot: 1000000
snapshot_prefix: "dqn"
